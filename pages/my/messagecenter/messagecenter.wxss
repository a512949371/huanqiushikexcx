/* pages/my/messagecenter/messagecenter.wxss */
@import '/app.wxss';
page{
  min-height: 100%;
  background-color: #f0f0f0;
}
.bgfff{
  background-color: #fff;
}
.page-box{
  padding: 0 40rpx;
}
.page-box .new-box{
  padding: 40rpx 0 0 0;
}
.page-box .new-box .time-box{
  text-align: center;
  font-size: 24rpx;
  padding-bottom: 40rpx;
  color: #969696;
}
.page-box .new-box .info-box{
  border-radius: 10rpx;
  padding: 20rpx;
}
.page-box .new-box .info-box .tit-box{
  font-size: 32rpx;
  color: #141414
}
.page-box .new-box .info-box .account{
  font-size: 24rpx;
  color: #b4b4b4;
}
.page-box .new-box .info-box .desc-box{
  font-size: 24rpx;
  color: #b4b4b4;
  padding: 10rpx 0;
}